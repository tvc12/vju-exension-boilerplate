{
  "name": "Example Extensions",
  "description": "Sample extension with vue and typescript",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "64": "assets/icon.png",
    "128": "assets/icon.png",
    "256": "assets/icon.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/icon.png",
      "32": "assets/icon.png",
      "64": "assets/icon.png",
      "128": "assets/icon.png",
      "256": "assets/icon.png"
    },
    "default_title": "Example Extensions"
  },
  "permissions": ["<all_urls>", "tabs", "activeTab"]
}
